<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_offical_info"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.bhash.govermentinfo.OfficalInfo">

    <TextView
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:id="@+id/t1"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:hint="@string/location"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/t2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/t1"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:textSize="18sp"
        android:hint="Name"
        tools:ignore="HardcodedText" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:id="@+id/t3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/t2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintHorizontal_bias="0.0"
        android:hint="Psoition"
        tools:ignore="HardcodedText" />

    <TextView
        android:layout_height="wrap_content"
        android:id="@+id/t4"
        android:hint="Party"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/t3"
        android:layout_width="0dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textStyle="normal|bold"
        android:textAlignment="center"
        android:textSize="18sp"
        tools:ignore="HardcodedText" />

    <ImageButton
        app:srcCompat="@color/cardview_dark_background"
        android:id="@+id/i1b1"
        android:layout_width="140dp"
        android:layout_height="140dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/t4"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/guideline2"
        android:contentDescription=""
        android:scaleType="fitXY"
        android:elevation="10dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="139dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="139dp" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/t5a1"
        android:textStyle="normal|bold"
        android:textSize="18sp"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        android:text="Address" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        app:layout_constraintGuide_begin="266dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="266dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/t6p1"
        android:textStyle="normal|bold"
        android:textSize="18sp"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toBottomOf="@+id/t5a1"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="Phone" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/t7e1"
        android:textStyle="normal|bold"
        android:textSize="18sp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/t6p1"
        android:text="Email" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/t8w1"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/t7e1"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textStyle="normal|bold"
        android:textSize="18sp"
        android:text="Website" />

    <TextView
        android:layout_height="wrap_content"
        android:id="@+id/t9a2"
        android:textSize="10sp"
        android:textStyle="normal|bold"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:maxLines="3"
        android:layout_width="220dp" />

    <TextView
        android:layout_height="wrap_content"
        android:id="@+id/t11e2"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/t10p2"
        android:layout_width="220dp" />

    <TextView
        android:layout_height="wrap_content"
        android:id="@+id/t10p2"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toBottomOf="@+id/t9a2"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        android:layout_marginStart="16dp"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:layout_width="180dp" />

    <TextView
        android:layout_height="wrap_content"
        android:id="@+id/t12w1"
        app:layout_constraintTop_toBottomOf="@+id/t11e2"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        android:layout_marginTop="8dp"
        android:layout_marginStart="16dp"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:layout_width="220dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@color/cardview_dark_background"
        android:id="@+id/i3b3"
        android:layout_marginStart="40dp"
        app:layout_constraintLeft_toRightOf="@+id/i2b2"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:contentDescription="" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@color/cardview_dark_background"
        android:id="@+id/i2b2"
        android:layout_marginStart="40dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@color/cardview_dark_background"
        android:id="@+id/i4b4"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="40dp"
        app:layout_constraintLeft_toRightOf="@+id/i3b3" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        app:srcCompat="@color/cardview_dark_background"
        android:id="@+id/i5b5"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="40dp"
        app:layout_constraintLeft_toRightOf="@+id/i4b4"
        android:contentDescription="" />
</android.support.constraint.ConstraintLayout>
